<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Health Index Calculator Suite">
    <listeners>
        <listener class-name="utilities.ExtentReportUtility"/>
    </listeners>
    <test name="Execution in Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="tests.FormUIValidationTest"></class>
            <class name="tests.FormFieldValidationTest"></class>
            <class name="tests.ResultUIValidationTest"></class>
            <class name="tests.ScoreValidationTest"></class>
        </classes>
    </test>
   </suite>